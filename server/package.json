{
  "name": "server",
  "version": "1.0.0",
  "main": "app.ts",
  "scripts": {
    "watch": "tsc --watch",
    "backend": "node --watch build/server/app.js",
    "dev": "concurrently \"npm run watch\" \"npm run backend\"",
    "build": "npm install && tsc && npm run migrate-up",
    "start": "node build/server/app.js",
    "start-prod": "pm2 start ecosystem.config.json --no-daemon",
    "migrate-up": "migrate-mongo up",
    "migrate-down": "migrate-mongo down",
    "drop-database": "cross-env NODE_ENV=development npx ts-node DB/scripts/drop-database.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.12",
    "eslint": "^9.11.1",
    "globals": "^15.9.0",
    "migrate-mongo": "^11.0.0",
    "typescript-eslint": "^8.7.0"
  },
  "dependencies": {
    "@hebcal/core": "^5.4.11",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^22.5.0",
    "axios-retry": "^4.5.0",
    "cors": "^2.8.5",
    "cron": "^3.1.7",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.12",
    "dotenv": "^16.4.5",
    "dotenv-flow": "^4.1.0",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "mongoose": "^8.5.4",
    "multer": "^1.4.5-lts.1",
    "play-sound": "^1.1.6",
    "socket.io": "^4.7.5",
    "typescript": "^5.5.4"
  },
  "keywords": [],
  "description": ""
}
